BEGIN;


CREATE TABLE IF NOT EXISTS public."Area"
(
    "Vehicle_Year" integer,
    "Violation_County" character varying COLLATE pg_catalog."default"
)
WITH (
    OIDS = FALSE
);

CREATE TABLE IF NOT EXISTS public."Vehicle Description"
(
    "Plate_Type" character varying COLLATE pg_catalog."default" NOT NULL,
    "Vehicle_Body_Type" character varying COLLATE pg_catalog."default",
    "Vehicle_Color" character varying COLLATE pg_catalog."default",
    "Vehicle_Year" integer,
    CONSTRAINT "Vehicle Description_pkey" PRIMARY KEY ("Plate_Type")
)
WITH (
    OIDS = FALSE
);

CREATE TABLE IF NOT EXISTS public."Violations"
(
    "Plate_Type" character varying COLLATE pg_catalog."default",
    "Violation_Code" integer,
    "Violation_Time" character varying,
    "Violation_County" character varying COLLATE pg_catalog."default"
)
WITH (
    OIDS = FALSE
);
END;

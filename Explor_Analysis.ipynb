    "# Fix Colors \n",
    "def change_color(color):\n",
    "  if color == 'GY':\n",
    "    return 'GREY'\n",
    "  elif color == 'BK':\n",
    "    return 'BLACK'\n",
    "  elif color == 'BLAK':\n",
    "    return 'BLACK'\n",
    "  elif color == 'WH':\n",
    "    return 'WHITE'\n",
    "  elif color == 'BL':\n",
    "    return 'BLUE'\n",
    "  elif color == 'RD':\n",
    "    return 'RED'\n",
    "  elif color == 'SILVE':\n",
    "    return 'SILVER'\n",
    "  elif color == 'GR':\n",
    "    return 'GREEN'\n",
    "  elif color == 'YW':\n",
    "    return 'YELLOW'\n",
    "  elif color == 'YELLO':\n",
    "    return 'YELLOW'\n",
    "  elif color == 'ORANG':\n",
    "    return 'ORANGE'\n",
    "  elif color == 'PURPL':\n",
    "    return 'PURPLE'\n",
    "  else: return color;\n",
    "    \n",
    "nyc_drop['Vehicle_Color']= nyc_drop['Vehicle_Color'].apply(change_color)\n",
    "nyc_drop"
   ]
